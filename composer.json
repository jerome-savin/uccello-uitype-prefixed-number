{
    "name": "jerome-savin/uccello-uitype-prefixed-number",
    "description": "An Uitype number-based with a customizable prefix/suffix",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Savin",
            "email": "jsavin@jsdev.fr"
        }
    ],
    "require": {
        "uccello/uccello": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "JeromeSavin\\UccelloUitypePrefixedNumber\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeromeSavin\\UccelloUitypePrefixedNumber\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeromeSavin\\UccelloUitypePrefixedNumber\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
